<odoo>
    <data noupdate="1">
        <!-- Sequences for scada test report -->
        <record id="seq_thoriumcorp_scada_test" model="ir.sequence">
            <field name="name">Scada Test</field>
            <field name="code">thoriumcorp.scada.test.result</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record>
        <!-- Sequences for scada request -->
        <record id="seq_thoriumcorp_scada_request" model="ir.sequence">
            <field name="name">Scada Request</field>
            <field name="code">thoriumcorp.patient.scada.test</field>
            <field name="prefix">LRQ</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>
